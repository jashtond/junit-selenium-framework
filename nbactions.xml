<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<actionName>CUSTOM-Compile</actionName>
		<displayName>Compile</displayName>
		<goals>
			<goal>clean</goal>
			<goal>install</goal>
			<goal>rox-junit:junit</goal>
			<goal>-e</goal>
		</goals>
	</action>
  <action>
    <actionName>CUSTOM-deploy</actionName>
    <displayName>Deploy</displayName>
    <goals>
      <goal>clean</goal>
      <goal>install</goal>
			<goal>deploy</goal>
      <goal>-e</goal>
    </goals>
  </action>
	<action>
		<actionName>CUSTOM-set-version</actionName>
		<displayName>Change Version</displayName>
		<goals>
			<goal>versions:set</goal>
			<goal>versions:commit</goal>
			<goal>-Pversioning</goal>
		</goals>
	</action>
	<action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.lotaris.selenium.TestMain -e</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/Users/ashton/Development/Selenium/junit-selenium-framework</exec.workingdir>
            </properties>
        </action>
	<action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath com.lotaris.selenium.TestMain -e</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>/Users/ashton/Development/Selenium/junit-selenium-framework</exec.workingdir>
            </properties>
        </action>
	<action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.lotaris.selenium.TestMain -e</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/Users/ashton/Development/Selenium/junit-selenium-framework</exec.workingdir>
            </properties>
        </action>
</actions>
